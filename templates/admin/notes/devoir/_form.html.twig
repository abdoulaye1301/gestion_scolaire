{{ form_start(form) }}
<!-- Main content -->
<section
	class="content">
	<!-- Informations personnelles -->
	<div
		class="box box-default">
		<!-- /.box-header -->
		<div class="row">
			<div class="box-body">
				<div class="col-md-6">
					<div class="form-group">
						{{ form_row(form.email) }}
					</div>
					<div class="form-group">
						{{ form_row(form.NomEvaluation) }}
					</div>
					<div class="form-group">
						{{ form_row(form.Matiere) }}
					</div>
				</div>
				<div class="col-md-4">
					<div class="form-group">
						{{ form_row(form.Note) }}
					</div>
					<div class="form-group">
						{{ form_row(form.Semestre) }}
					</div>
					<div class="form-group">
						{{ form_row(form.DateDevoir) }}
					</div>
				</div>
			</div>
		</div>
	</div>

</section>
<center>
	<button class="btn btn-primary">{{ button_label|default('Enregistrer') }}</button>
</center>
{{ form_end(form) }}
