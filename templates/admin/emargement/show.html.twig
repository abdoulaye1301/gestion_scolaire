{% extends 'admin/Dashboard/administration1.html.twig' %}

{% block title %}Emargement
{% endblock %}

{% block body %}
	<h1>Emargement</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ emargement.id }}</td>
			</tr>
			<tr>
				<th>TitreCours</th>
				<td>{{ emargement.TitreCours }}</td>
			</tr>
			<tr>
				<th>DateEmargement</th>
				<td>{{ emargement.DateEmargement ? emargement.DateEmargement|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>
	<div class="row">
		<div class="col-lg-4">
			<a href="{{ path('app_emargement_index') }}">
				<i class="glyphicon glyphicon-chevron-left"></i>
				<span>Retour
				</span>
			</a>
		</div>
		<div class="col-lg-4">
			<a href="{{ path('app_emargement_edit', {'id': emargement.id}) }}">
				<i class="glyphicon glyphicon-pencil"></i>
				<span>Modifier
				</span>
			</a>
		</div>
		<div class="col-lg-4">
			{{ include('admin/emargement/_delete_form.html.twig') }}
		</div>
	</div>
{% endblock %}
